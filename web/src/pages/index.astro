---
import Layout from '../layouts/Layout.astro';
import { getEntryBySlug } from 'astro:content';
const page = await getEntryBySlug('pages', 'accueil');
if (!page) throw new Error('Page accueil introuvable');
const { Content } = await page.render();
---
<Layout title="Accueil - Maud Pozza, psychologue Ã  Angers">
  <article class="prose prose-slate max-w-none">
    <h1 class="text-3xl font-semibold text-[#423560]">{page.data.title}</h1>
    <Content />
  </article>
</Layout>
